{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": ["google","prettier"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "plugins": ["prettier"],
  "rules": {
  "prettier/prettier": "error",
  "complexity": ["error", { "max": 2 }]
  },
  "ignorePatterns" : [
    ",./*.config.js"
  ]
}
